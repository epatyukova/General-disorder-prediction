{
    "seed": 42,
    "model_name": "roost",
    "epochs": 50,
    "patience": 10,
    "test_size": 0.2,
    "val_size": 0.1,
    "classification": true,
    "data_params": {
        "batch_size": 128,
        "train_path": "data/roost_data_3classes/train.csv",
        "val_path": "data/roost_data_3classes/val.csv",
        "test_path": "data/roost_data_3classes/test.csv"
    },
    "setup_params": {
        "optim": "AdamW",
        "learning_rate": 0.0005,
        "weight_decay": 1e-06,
        "momentum": 0.9,
        "loss": "BCEWithLogitsLoss",
        "base_lr": 0.0001,
        "max_lr": 0.006,
        "swa_epoch_start": 0.05,
        "swa_lrs": 0.01
    },
    "model_params": {
        "input_dim": "elem_emb_len",
        "output_dim": 3,
        "hidden_layer_dims": [
            1024,
            512,
            256,
            64
        ],
        "n_graphs": 2,
        "elem_heads": 3,
        "internal_elem_dim": 64,
        "g_elem_dim": 256,
        "f_elem_dim": 256,
        "comp_heads": 3,
        "g_comp_dim": 256,
        "f_comp_dim": 256,
        "batchnorm": false,
        "negative_slope": 0.2
    }
}